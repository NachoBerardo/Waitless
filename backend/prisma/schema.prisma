generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Food {
  idFood      Int    @id @default(autoincrement())
  category    String
  name        String
  price       Int
  description String
  image       String
  video       String
}
